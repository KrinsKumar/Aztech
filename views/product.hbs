   <div class="h-full px-9 py-4 overflow-y-auto border-r border-gray-200 bg-gray-50">
        <h3 class="my-3 text-xl font-bold text-urban">Products</h3>
        <!-- Search Bar  -->
        <form>   
            <label for="default-search" class="mb-2 text-sm font-medium text-gray-900 sr-only">Search</label>
            <div class="relative">
                <input type="search" id="default-search" class="block w-full p-4 text-sm text-gray-900 border border-gray-300 rounded-lg bg-gray-50 focus:ring-blue-500 focus:border-blue-500" placeholder="Search..." required>
                <button type="submit" class="text-white absolute right-2.5 bottom-2.5 bg-urban hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-3 py-2">
                    <svg aria-hidden="true" class="w-5 h-5 text-gray-50" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z"></path></svg>
                </button>
            </div>
        </form>
        <ul class="mt-4 space-y-2 font-medium">
            <li>
                <div class="flex items-center mb-4">
                    <input id="stocked-checkbox" type="checkbox" value="" class="w-4 h-4 text-urban bg-gray-100 border-gray-300 rounded focus:ring-urban">
                    <label for="stocked-checkbox" class="ml-2 text-sm font-medium text-gray-900">In Stock</label>
                </div>
            </li>
        </ul>

        <ul class="pt-4 mt-4 space-y-1 font-medium border-t border-gray-200">
            <h3 class="my-3 text-xl font-bold text-urban">Categories</h3>

            <li>
            <button type="button" class="flex items-center w-full p-2 text-gray-900 transition duration-75 rounded-lg group hover:bg-gray-100 " aria-controls="dropdown-example" data-collapse-toggle="dropdown-example">
                <span class="flex-1 ml-1 text-left whitespace-nowrap" sidebar-toggle-item >Fire Detection and Alarm</span>
                <svg sidebar-toggle-item class="w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z" clip-rule="evenodd"></path></svg>
            </button>
            <ul id="dropdown-example" class="hidden py-2 space-y-2">
                <li>
                    <a href="#" class="flex items-center w-full p-2 text-gray-500 transition duration-75 rounded-lg pl-8 group hover:bg-gray-100">FX-4000 Intelligent Fire Control Unit</a>
                </li>
                <li>
                    <a href="#" class="flex items-center w-full p-2 text-gray-500 transition duration-75 rounded-lg pl-8 group hover:bg-gray-100">Remote Annunciator Enclosures</a>
                </li>
                <li>
                    <a href="#" class="flex items-center w-full p-2 text-gray-500 transition duration-75 rounded-lg pl-8 group hover:bg-gray-100">MGC-4000 Sensors and Modules</a>
                </li>
                <li>
                    <a href="#" class="flex items-center w-full p-2 text-gray-500 transition duration-75 rounded-lg pl-8 group hover:bg-gray-100">Mircom 800 Manual Plastic Pull Stations</a>
                </li>
            </ul>
            </li>
            <li>
                <a href="#" class="flex items-center p-2 text-gray-900 transition duration-75 rounded-lg hover:bg-gray-100">
                    <span class="ml-1">Communications and Security</span>
                </a>
            </li>
            <li>
                <a href="#" class="flex items-center p-2 text-gray-900 transition duration-75 rounded-lg hover:bg-gray-100">
                    <span class="ml-1">Mass Notification</span>
                </a>
            </li>
            <li>
                <a href="#" class="flex items-center p-2 text-gray-900 transition duration-75 rounded-lg hover:bg-gray-100">
                    <span class="ml-1">Smart Buildings</span>
                </a>
            </li>
            
        </ul>
    </div>
</aside>


<div class="p-4 pl-10 -z-50 sm:ml-72">
    <!-- Pathways -->
    <h3 class="my-3 ml-2 text-2xl font-bold text-gray-900">Pathways</h3>
    <div class="flex flex-row">
        <div class="max-w-sm w-1/2 bg-white border border-gray-200 rounded-lg shadow mx-2 bg-center bg-no-repeat bg-[url('/photo/fx-400.png')]">
            <div class="p-5 bottom-0">
                <h5 class="mb-2 text-xl font-bold tracking-tight text-gray-900">4000 Series Pathway</h5>
                <p class="mb-3 font-normal text-sm text-gray-700">Sensors and Detectors</p>
                <a href="/pathway/fx-400/1" class="inline-flex items-center px-3 py-2 text-sm font-medium text-center text-white bg-urban rounded-lg hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300">
                    Guide Me
                    <svg aria-hidden="true" class="w-4 h-4 ml-2 -mr-1" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M10.293 3.293a1 1 0 011.414 0l6 6a1 1 0 010 1.414l-6 6a1 1 0 01-1.414-1.414L14.586 11H3a1 1 0 110-2h11.586l-4.293-4.293a1 1 0 010-1.414z" clip-rule="evenodd"></path></svg>
                </a>
            </div>
        </div>

        <div class="max-w-sm w-1/2 bg-white border border-gray-200 rounded-lg shadow mx-2 bg-right bg-no-repeat bg-[url('/photo/sensors.png')]">
            <div class="p-5">
                <h5 class="mb-2 text-xl font-bold tracking-tight text-gray-900">FX-400 Series Pathway</h5>
                <p class="mb-3 font-normal text-sm text-gray-700">Fire alarm contorl unit</p>
                <a href="/pathway/4k/1" class="inline-flex items-center px-3 py-2 text-sm font-medium text-center text-white bg-urban rounded-lg hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300">
                    Guide Me
                    <svg aria-hidden="true" class="w-4 h-4 ml-2 -mr-1" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M10.293 3.293a1 1 0 011.414 0l6 6a1 1 0 010 1.414l-6 6a1 1 0 01-1.414-1.414L14.586 11H3a1 1 0 110-2h11.586l-4.293-4.293a1 1 0 010-1.414z" clip-rule="evenodd"></path></svg>
                </a>
                <br><br><br><br><br><br>
            </div>
        </div>
    </div>

    
    {{#each catData}}
        <h3 class="my-3 ml-2 text-2xl font-bold text-gray-900">{{categoryName}}</h3>
        <div class="flex items-center justify-center w-full h-full py-24 sm:py-8 px-4">
            <div class="w-full relative flex items-center justify-center">
                <button aria-label="slide backward" class="absolute z-30 left-0 ml-10 focus:outline-none focus:bg-gray-400 focus:ring-2 focus:ring-offset-2 focus:ring-gray-400 cursor-pointer" id="prev">
                    <svg class="dark:text-gray-900" width="8" height="14" viewBox="0 0 8 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M7 1L1 7L7 13" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
                    </svg>
                </button>
                <div class="w-full h-full mx-auto overflow-x-hidden overflow-y-hidden">
                    <div id="slider" class="h-full flex lg:gap-8 md:gap-6 gap-14 items-center justify-start transition ease-out duration-700">
                            {{#each this.products}}
                                <div class="flex flex-shrink-0 relative sm:w-auto overflow-hidden">
                                    <img src="{{image}}" alt="{{productName}}" class="object-cover object-center overflow-hidden" />
                                    <div class="bg-gray-800 bg-opacity-30 absolute w-full h-full p-6">
                                        <h2 class="lg:text-xl leading-4 text-base lg:leading-5 text-white dark:text-gray-900">{{productName}}</h2>
                                        <div class="flex h-full items-end pb-6">
                                            <h3 class="text-xl lg:text-2xl font-semibold leading-5 lg:leading-6 text-white dark:text-gray-900">{{description}}</h3>
                                        </div>
                                    </div>
                                </div>
                            {{/each}}
                    </div>
                </div>
                <button aria-label="slide forward" class="absolute z-30 right-0 mr-10 focus:outline-none focus:bg-gray-400 focus:ring-2 focus:ring-offset-2 focus:ring-gray-400" id="next">
                    <svg class="dark:text-gray-900" width="8" height="14" viewBox="0 0 8 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M1 1L7 7L1 13" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
                    </svg>
                </button>
            </div>
        </div>
    {{/each}}
      
        
        
    

